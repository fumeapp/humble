{
  "name": "acidjazz/humble",
  "type": "library",
  "license": "MIT",
  "description": "passwordless authentication and detailed sessioning for laravel",
  "keywords": [
    "Laravel", "API", "authentication", "passwordless", "PHP"
  ],
  "authors": [
    {
      "name": "Kevin Olson",
      "email": "acidjazz@gmail.com"
    }
  ],
  "require": {
    "illuminate/support": "8.*|9.*",
    "php": "^8.0|^8.1",
    "whichbrowser/parser": "^2.1",
    "torann/geoip": "^3.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^6.2",
    "laravel/pint": "^0.2.1",
    "phpunit/phpunit": "^8.0|^9.3"
  },
  "autoload": {
    "psr-4": {
     "acidjazz\\Humble\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "acidjazz\\Humble\\HumbleServiceProvider"
      ]
    }
  },
  "scripts": {
    "pint": [
      "vendor/bin/pint"
    ],
    "test": [
      "vendor/bin/phpunit"
    ]
  },
  "scripts-descriptions": {
    "pint": "Run the Pint Linter and Fixer.",
    "test": "Run the PHPUnit tests."
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
